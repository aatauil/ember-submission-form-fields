{{#each this.propertyGroups as |group index|}}
    <PropertyGroup
            @form={{@form}}
            @field={{group}}
            @formStore={{@formStore}}
            @graphs={{@graphs}}
            @sourceNode={{@sourceNode}}
            @forceShowErrors={{@forceShowErrors}}
            @cacheConditionals={{@cacheConditionals}}
            @show={{@show}}/>
{{/each}}