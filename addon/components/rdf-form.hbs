{{#each this.propertyGroups as |group index|}}
    <PropertyGroup
            @form={{@form}}
            @field={{group}}
            @formStore={{@formStore}}
            @graphs={{@graphs}}
            @sourceNode={{@sourceNode}}
            @forceShowErrors={{@forceShowErrors}}
            @show={{@show}}/>
    {{! -- #unless (eq index (sub this.propertyGroups.length 1))
      <hr class="au-c-hr">
    unless --}}
{{/each}}